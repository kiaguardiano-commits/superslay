    <script>
        // Category filter functionality
        document.querySelectorAll('.category-filter button').forEach(button => {
            button.addEventListener('click', () => {
                const category = button.dataset.category;
                
                // Update active state
                document.querySelectorAll('.category-filter button').forEach(btn => {
                    btn.classList.remove('active');
                    btn.classList.add('bg-gray-200');
                });
                button.classList.add('active');
                button.classList.remove('bg-gray-200');
                
                // Filter products
                document.querySelectorAll('.product-card').forEach(card => {
                    if (category === 'all' || card.dataset.category === category) {
                        card.style.display = 'block';
                    } else {
                        card.style.display = 'none';
                    }
                });
            });
        });

        // Product click tracking for analytics (placeholder)
        document.querySelectorAll('.product-card button').forEach(button => {
            button.addEventListener('click', () => {
                console.log(`Product clicked: ${button.parentElement.parentElement.querySelector('h3').textContent}`);
                // In a real implementation, you would track this with your analytics platform
            });
        });
    </script>
